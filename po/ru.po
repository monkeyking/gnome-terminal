msgstr "_Основать новый профиль на:"

#: ../src/profile-preferences.glade.h:1
msgid "<b>Command</b>"
msgstr "<b>Команда</b>"

#: ../src/profile-preferences.glade.h:2
msgid "<b>Foreground and Background</b>"
msgstr "<b>Текст и фон</b>"

