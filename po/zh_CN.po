msgid "GNOME Terminal Client"
msgstr "GNOME终端"

#: ../src/client.c:395
msgid "Global options:"
msgstr "全局选项："

#: ../src/client.c:396
msgid "Show global options"
msgstr "显示全局选项"

#: ../src/client.c:404
msgid "Server options:"
msgstr "服务器端选项："

#: ../src/client.c:405
msgid "Show server options"
msgstr "显示服务器端选项"

#: ../src/client.c:413
msgid "Window options:"
msgstr "窗口选项："

#: ../src/client.c:414
msgid "Show window options"
msgstr "显示窗口选项"

#: ../src/client.c:422
msgid "Terminal options:"
msgstr "终端选项："

