msgstr "转发标准输出"

#: ../src/client.c:373
msgid "Forward stderr"
msgstr "转发标准错误"

#: ../src/client.c:375
msgid "Forward file descriptor"
msgstr "转发文件描述符"

#: ../src/client.c:375
msgid "FD"
msgstr "文件描述符"

#: ../src/client.c:381
msgid "Wait until the child exits"
msgstr "等待子进程退出"

