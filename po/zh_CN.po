msgid "Show terminal options"
msgstr "显示终端选项"

#: ../src/client.c:431
msgid "Exec options:"
msgstr "Exec 选项："

#: ../src/client.c:432
msgid "Show exec options"
msgstr "显示 Exec 选项"

#: ../src/client.c:440
msgid "Processing options:"
msgstr "进程管理选项："

#: ../src/client.c:441
